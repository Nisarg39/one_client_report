{
  "permissions": {
    "allow": [
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git branch:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(npm:*)",
      "Bash(node:*)",
      "Bash(npx tsx:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(kill:*)",
      "Bash(lsof:*)",
      "Bash(xargs:*)",
      "Read(**/*.ts:*)",
      "Read(**/*.tsx:*)",
      "Read(**/*.js:*)",
      "Read(**/*.jsx:*)",
      "Read(**/*.json:*)",
      "Read(**/*.md:*)",
      "Read(.env.example:*)",
      "Read(package.json:*)",
      "Read(tsconfig.json:*)",
      "Read(prisma/schema.prisma:*)",
      "Write(**/*.ts:*)",
      "Write(**/*.tsx:*)",
      "Write(**/*.js:*)",
      "Write(**/*.jsx:*)",
      "Write(**/*.json:*)",
      "Write(**/*.md:*)"
    ],
    "deny": [
      "Read(.env:*)",
      "Read(.env.local:*)",
      "Read(.env.production:*)",
      "Read(**/.env:*)",
      "Read(**/credentials.json:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(rm -rf /*:*)",
      "Write(.env:*)",
      "Write(.env.local:*)"
    ],
    "ask": [
      "Bash(git push:*)",
      "Bash(npm run build:*)",
      "Bash(npm run deploy:*)",
      "Bash(npm publish:*)",
      "Bash(rm:*)"
    ]
  },
  "env": {
    "NODE_ENV": "development"
  }
}
